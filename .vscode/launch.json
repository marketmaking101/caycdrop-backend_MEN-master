{
  "configurations": [
  {
    "console": "integratedTerminal",
    "internalConsoleOptions": "neverOpen",
    "name": "nodemon",
    "program": "${workspaceFolder}/app.js",
    "request": "launch",
    "restart": true,
    "runtimeExecutable": "node",
    "skipFiles": [
      "<node_internals>/**"
    ],
    "type": "node"
  },
    {
      "type": "chrome",
      "name": "http://localhost:5000",
      "request": "launch",
      "url": "http://localhost:5000"
    }
  ]
}